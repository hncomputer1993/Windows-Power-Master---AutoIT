Func _UnmaskMysticPower()
Local $sKeyBase = "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Power\PowerSettings\54533251-82be-4824-96c1-47b60b740d00\"
Local $aKeys[5] = [ "06cadf0e-64ed-448a-8927-ce7bf90eb35d", "12a0ab44-fe28-4fa9-b3bd-4b64f44960a6", "bc5038f7-23e0-4960-96da-33abaf5935ec", "893dee84-a2e9-44df-82e1-450be8406211", "94d3a615-a899-4ac5-ad2c-e063069e545c"]
For $i = 0 To 4
RegWrite($sKeyBase & $aKeys[$i], "Attributes", "REG_DWORD", 2)
Next
Local $iRes = MsgBox(36 + 256, "Gii Ba Hng", "Bc 1:  hin cc menu chnh CPU b hng n i." & @CRLF & @CRLF & "Bc 2: Bn c mun TT LUN ch  'Modern Standby' (S0) khng?" & @CRLF & "--------------------------------------------------" & @CRLF & "[YES] = Tt S0 (Khuyn dng cho PC/Laptop Gaming)." & @CRLF & "[NO] = Ch hin menu CPU (Gi nguyn ch  ng mc nh)." & @CRLF)
If $iRes = 6 Then
RegWrite("HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Power", "CsEnabled", "REG_DWORD", 0)
RegWrite("HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Power", "PlatformAoAcOverride", "REG_DWORD", 0)
MsgBox(64, "Thnh cng", " gii ba hon tt! Vui lng KHI NG LI MY  p dng.")
Else
MsgBox(64, "Thnh cng", " hin cc ci t CPU. Bn c th vo Power Options  chnh th cng.")
EndIf
EndFunc
Func _UpdateHibButtonState()
If FileExists("C:\hiberfil.sys") Then
GUICtrlSetData($btHB, "Tt Tnh Nng Ng ng")
Else
GUICtrlSetData($btHB, "Bt Tnh Nng Ng ng")
EndIf
EndFunc
Func _ToggleHibernate()
If FileExists("C:\hiberfil.sys") Then
Local $iRes = MsgBox(36, "Xc nhn", "Tnh nng ang BT." & @CRLF & "Bn c mun TT  gii phng  cng khng?")
If $iRes = 6 Then
RunWait("powercfg -h off", "", @SW_HIDE)
_UpdateHibButtonState()
MsgBox(64, "Xong", " TT tnh nng ng ng.")
EndIf
Else
Local $iRes = MsgBox(36, "Xc nhn", "Tnh nng ang TT." & @CRLF & "Bn c mun BT li  khi ng nhanh hn khng?")
If $iRes = 6 Then
RunWait("powercfg -h on", "", @SW_HIDE)
_UpdateHibButtonState()
MsgBox(64, "Xong", " BT tnh nng ng ng.")
EndIf
EndIf
EndFunc
Func _GetActiveScheme()
Local $iPID = Run("powercfg /getactivescheme", "", @SW_HIDE, $STDOUT_CHILD)
ProcessWaitClose($iPID)
Local $sOutput = StdoutRead($iPID)
Local $aArray = StringRegExp($sOutput, "([a-f0-9]{8}-([a-f0-9]{4}-){3}[a-f0-9]{12})", 3)
If IsArray($aArray) Then
Return $aArray[0]
Else
Return ""
EndIf
EndFunc
Func _SetPowerSetting($sScheme, $sSubGroup, $sSetting, $iValueAC, $iValueDC)
RunWait('powercfg /SETACVALUEINDEX ' & $sScheme & ' ' & $sSubGroup & ' ' & $sSetting & ' ' & $iValueAC, "", @SW_HIDE)
RunWait('powercfg /SETDCVALUEINDEX ' & $sScheme & ' ' & $sSubGroup & ' ' & $sSetting & ' ' & $iValueDC, "", @SW_HIDE)
EndFunc
Func _SetMaxPerformance()
Local $sActive = _GetActiveScheme()
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MIN_STATE, 100, 100)
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MAX_STATE, 100, 100)
_SetPowerSetting($sActive, $GUID_DISK_SUBGROUP, $GUID_DISK_TIMEOUT, 0, 0)
_SetPowerSetting($sActive, $GUID_SLEEP_SUBGROUP, $GUID_SLEEP_TIMEOUT, 0, 0)
_SetPowerSetting($sActive, $GUID_PCI_SUBGROUP, $GUID_PCI_EXPRESS, 0, 0)
_SetPowerSetting($sActive, $GUID_WIFI_SUBGROUP, $GUID_WIFI_SETTING, 0, 0)
RunWait('powercfg /setactive ' & $sActive, "", @SW_HIDE)
EndFunc
Func _SetBalancedMode()
Local $sActive = _GetActiveScheme()
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MIN_STATE, 5, 5)
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MAX_STATE, 100, 100)
_SetPowerSetting($sActive, $GUID_DISK_SUBGROUP, $GUID_DISK_TIMEOUT, 20, 10)
_SetPowerSetting($sActive, $GUID_PCI_SUBGROUP, $GUID_PCI_EXPRESS, 1, 2)
_SetPowerSetting($sActive, $GUID_WIFI_SUBGROUP, $GUID_WIFI_SETTING, 0, 2)
_SetPowerSetting($sActive, $GUID_DISPLAY_SUBGROUP, $GUID_BRIGHTNESS_LEVEL, 90, 75)
RunWait('powercfg /setactive ' & $sActive, "", @SW_HIDE)
EndFunc
Func _SetPowerSaverMode()
Local $sActive = _GetActiveScheme()
_SetPowerSetting($sActive, $GUID_DISPLAY_SUBGROUP, $GUID_BRIGHTNESS_LEVEL, 70, 50)
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MIN_STATE, 0, 0)
_SetPowerSetting($sActive, $GUID_CPU_SUBGROUP, $GUID_CPU_MAX_STATE, 70, 50)
_SetPowerSetting($sActive, $GUID_DISK_SUBGROUP, $GUID_DISK_TIMEOUT, 10, 5)
_SetPowerSetting($sActive, $GUID_PCI_SUBGROUP, $GUID_PCI_EXPRESS, 2, 2)
_SetPowerSetting($sActive, $GUID_WIFI_SUBGROUP, $GUID_WIFI_SETTING, 2, 3)
RunWait('powercfg /setactive ' & $sActive, "", @SW_HIDE)
EndFunc
Func _SetFanPolicy($iMode)
Local $sActive = _GetActiveScheme()
RunWait('powercfg /SETACVALUEINDEX ' & $sActive & ' ' & $GUID_CPU_SUBGROUP & ' ' & $GUID_COOLING_POLICY & ' ' & $iMode, "", @SW_HIDE)
RunWait('powercfg /SETDCVALUEINDEX ' & $sActive & ' ' & $GUID_CPU_SUBGROUP & ' ' & $GUID_COOLING_POLICY & ' ' & $iMode, "", @SW_HIDE)
RunWait('powercfg /setactive ' & $sActive, "", @SW_HIDE)
EndFunc
Func _UnlockUltimate()
RunWait("powercfg -duplicatescheme e9a42b02-d5df-448d-aa00-03f14749eb61", "", @SW_HIDE)
MsgBox(64, "Xong", " thm profile 'Ultimate Performance' vo Power Options.")
EndFunc
